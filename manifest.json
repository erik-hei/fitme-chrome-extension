{
    "manifest_version": 2,
    "name": "FitMe",
    "description": "An extension to help users filter clothing reviews based on size",
    "version": "1.0.0",
    "icons": {"128": "icon.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}